{
  "name": "VILLAIN_MUSIC",
  "description": "A Telegram Music Player Bot, written in Python with Pyrogram and Py-Tgcalls.",
  "logo": "https://files.catbox.moe/bswmmi.jpg",
  "keywords": [
    "python3",
    "telegram",
    "bot",
    "VILLAIN",
    "MusicBot",
    "telegram-bot",
    "pyrogram"
  ],
  "env": {
    "API_ID": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "API_HASH": {
      "description": "Get this value from https://my.telegram.org",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "A Bot's token from BotFather",
      "required": true
    },
    "MONGO_DB_URI": {
      "description": "Get a mongodb url from https://cloud.mongodb.com.",
      "required": true
    },
    "OWNER_ID": {
      "description": "Your Telegram numeric user ID.",
      "required": true
    },
    "STRING_SESSION": {
      "description": "A Pyrogram v2 String Session from @KING_STRING_SESSION_BOT on Telegram.",
      "required": true
    },
    "HEROKU_API_KEY": {
      "description": "Your Heroku account's API key",
      "required": false
    },
    "HEROKU_APP_NAME": {
      "description": "Your Heroku app name",
      "required": false
    },
    "LOGGER_ID": {
      "description": "Your Log Group ID. Do not use a channel ID.",
      "required": true
    }
  },
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ],
  "stack": "container"
}
